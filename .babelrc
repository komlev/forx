{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "ie > 9", "not ie <= 9", "safari > 8", "not safari <= 8"]
      },
      "modules": false
    }],
    "stage-2"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "modules": "commonjs"
        }]
      ]
    },
    "production": {
      "plugins": ["lodash"]
    }
  }
}
